   ||
   (family OTOut[bound N + 2 bound N + 2 bound K]  
     indices: n, m, k 
     bounds: bound N + 2 bound N + 2 bound K ::=
     m0 : bool <- read OTMsg-0[n, m, k] ;
     m1 : bool <- read OTMsg-1[n, m, k] ;
     m2 : bool <- read OTMsg-2[n, m, k] ;
