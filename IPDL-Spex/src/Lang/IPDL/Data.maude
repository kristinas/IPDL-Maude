fmod IPDL/MOD is
  protecting IPDL/DECL .

  var  L : IPDL .
  vars I S : Qid .
  var  DL : DeclList{IPDL} .
  ***(
  op add : DeclList{IPDL} Qid -> DeclList{IPDL} .
  eq add(nil, S) = nil .
  eq add(Decl[L]{type I .} DL, S)
  = Decl[L]{type qid(I S) .} add(DL, S).
  )
endfm

