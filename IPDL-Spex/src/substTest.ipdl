lang IPDL

channel context delta = 
input I1 : bool ;
input I2 : bool ;
output C : bool ;
output D : bool ; 
output E : bool .

protocol P =
 (C ::= i1 : bool <- read I1 ; return i1)
 ||
 (D ::= i2 : bool <- read I2 ; return i2)
 ||
 (E ::= c : bool <- read C ; d : bool <- read D ; return True)
.

start with P over delta

