lang IPDL

parameter N : nat .

channel context delta =
 output F[bound N bound N] : bool ;
 output G[bound N bound N] : bool
.

protocol P = 
 family Test[bound N bound N] 
 indices: m, n
  bounds: bound N bound N ::=
 (
 (family F[bound N bound N]  
  indices: m, n
  bounds: bound N bound N ::= return True) 
 || 
 (family G[bound N bound N]  
  indices: m, n
  bounds: bound N bound N ::= return True )
 ) 
.

start with P over delta

save current configuration

current-protocol
